{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 7
    },
    "rules": {
        "no-undef": "warn",
        "no-console":"off",
        "newline-before-return":"warn",
        "newline-after-var":"warn",
        "no-multiple-empty-lines":"warn",
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ]
    }
}